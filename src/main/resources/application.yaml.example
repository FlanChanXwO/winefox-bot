spring:
  application:
    name: wine-fox-bot-app
  threads:
    virtual:
      enabled: true
  sql:
    init:
      # 开启/关闭数据库初始化 (always, embedded, never)
      # always: 总是执行脚本
      # embedded: 只对内存数据库 (H2, HSQLDB, Derby) 执行
      # never: 从不执行
      mode: always
      # 如果初始化过程中发生错误，是否继续。建议设置为 false，以便及时发现问题。
      continue-on-error: false
      schema-locations: classpath:db/schema-postgres.sql
  datasource:
#    driver-class-name: org.sqlite.JDBC
    type: com.zaxxer.hikari.HikariDataSource
    driver-class-name: org.postgresql.Driver
#    url: jdbc:sqlite:db/winefoxbot.db
    url: jdbc:postgresql://127.0.0.1:5432/winefoxbot
    username: postgres
    password: postgres123
    hikari:
      auto-commit: true
      connection-timeout: 30000
      maximum-pool-size: 10

  ai:
    deepseek:
      api-key: sk-deepseek
      base-url: http://127.0.0.1:5001
      chat:
        options:
          model: deepseek-chat-search
        completions-path: /v1/chat/completions

    openai:
      api-key: sk-4nU9k3QyVpX7wJgRz2t5sD1mA8oF6iE
      base-url: http://127.0.0.1:5002
      chat:
        options:
          model: grok-4.1
        completions-path: /v1/chat/completions
