jobrunr:
  background-job-server:
    enabled: true
  job-scheduler:
    enabled: true
  dashboard:
    enabled: false
mybatis-plus:
  mapper-locations: classpath:/mapper/**/*.xml
  configuration:
    log-impl: org.apache.ibatis.logging.stdout.StdOutImpl
    map-underscore-to-camel-case: true
  type-aliases-package: com.github.winefoxbot.model.entity
server:
  port: 5000
nonebot2:
  enabled: false
  cmd: nb run --reload
  bot-path: D:\SourceCode\GithubProjects\winefox-bot\winefox-bot-nonebot2
winefox:
  bot:
    - 123
  master: 3085974224
  adminUsers:
    - 123
  allow-groups:
    - 123
  block-commands:
    - "^/清空会话$"
    - "^\\/echo\\s.+$"
    - "^/测试欢迎消息$"
    - "^/测试离开消息$"
    - "^/测试停止消息$"
    - "^/version$"
    - "^/sendGroupMsg\\s.+$"
    - "^/sendPrivateMsg\\s.+$"
  test-path: /opt/software/audio.mp3

webdriver:
  edgeDriverPath: S:/Program Files/WebDriver/edgedriver_win64/msedgedriver.exe
  edgeBinaryPath: C:/Program Files (x86)/Microsoft/Edge/Application/msedge.exe

okhttp:
  proxy:
    type: socks5
    host: 127.0.0.1
    port: 7889
    enabled: on

shiro:
  task-pool:
    enable-task-pool: false
  ws:
    server:
      url: /ws/shiro
      enable: true
    client:
      reconnect-interval: 5
    access-token: suCiTU_{o.JSGeq<
    timeout: 30
  pluginScanPath: "plugins"
  interceptor: com.github.winefoxbot.aop.interceptor.BotReceiveMsgInterceptor
logging:
  level:
    root: info

